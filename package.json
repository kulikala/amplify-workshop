{
  "name": "amplify-workshop",
  "version": "1.0.0",
  "description": "Sample app based on AWS Amplify Workshop with Vue.js",
  "author": "Kaz Namba <kaz.namba@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "clean": "rimraf .nuxt dist",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@aws-amplify/core": "^2.2.2",
    "aws-amplify": "^2.2.2",
    "aws-amplify-vue": "^1.1.2",
    "nuxt": "^2.11.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/vuetify": "^2.0.0-alpha.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-nuxt": ">=0.5.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^3.0.1"
  }
}
